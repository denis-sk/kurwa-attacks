services:
  kurwa-attacks:
    build:
      context: /Users/denisskvortsov/Projects/KurwaAttacks
      dockerfile: kurwa-attacks/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: kurwa-attacks
      target: base
    container_name: kurwa-attacks
    entrypoint:
    - tail
    - -f
    - /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      DOTNET_USE_POLLING_FILE_WATCHER: "1"
      NUGET_FALLBACK_PACKAGES: ""
    image: kurwaattacks:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/net6.0/kurwa-attacks.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      default: null
    ports:
    - mode: ingress
      target: 80
      protocol: tcp
    tty: true
    volumes:
    - type: bind
      source: /Users/denisskvortsov/Projects/KurwaAttacks/kurwa-attacks
      target: /app
      bind:
        create_host_path: true
    - type: bind
      source: /Users/denisskvortsov/Projects/KurwaAttacks
      target: /src
      bind:
        create_host_path: true
    - type: bind
      source: /Users/denisskvortsov/vsdbg/vs2017u5
      target: /remote_debugger
      bind:
        create_host_path: true
    - type: bind
      source: /Users/denisskvortsov/.nuget/packages/
      target: /root/.nuget/packages
      read_only: true
      bind:
        create_host_path: true
networks:
  default:
    name: dockercompose1936644245388529437_default